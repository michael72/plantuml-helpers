{
  "reporter": [
    "html",
    "text",
    "text-summary",
    "json",
    "lcov"
  ],
  "check-coverage": true,
  "all": true,
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "**/*.d.ts",
    "**/*.spec.ts",
    "**/*.js",
    "out/**",
    "coverage/**",
    "test/**",
    "node_modules/**"
  ],
  "extension": [
    ".ts"
  ],
  "statements": 80,
  "branches": 70,
  "functions": 80,
  "lines": 80
}